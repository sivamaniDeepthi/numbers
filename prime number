n=int(input())
for i in range(2,n):
    if(n%i==0):
        print("not prime")
        break
else:
    print("prime")

-------------------------------------------

#include <iostream>
using namespace std;
int main() {
    int n;
    cin>>n;
    if(n<=1){
        cout<<"Not prime";
         return 0;
    }
    for(int i=2;i<n;++i){
        if(n%i==0){
            cout<<"Not Prime";
             return 0;
        }
    }cout<<"Prime";

    return 0;
}

---------------------------------------------
Input
6
output
Not Prime
